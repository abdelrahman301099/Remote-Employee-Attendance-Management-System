<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <Compile Update="SharedResources\Messages.ar-EG.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Messages.ar-EG.resx</DependentUpon>
    </Compile>
    <Compile Update="SharedResources\StaticLocalization.ar-EG.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>StaticLocalization.ar-EG.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="SharedResources\Messages.ar-EG.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Messages.ar-EG.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="SharedResources\StaticLocalization.ar-EG.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>StaticLocalization.ar-EG.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
